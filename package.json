{
  "name": "blender-config-manager",
  "version": "1.0.0",
  "description": "Blender Configuration Manager",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-main && npm run build-render",
    "start-main": "cross-env HOT=1 electron --enable-logging ./electron/main",
    "start-render": "cross-env NODE_ENV=development node --trace-warnings --max_old_space_size=2096 -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js --progress --color --watch",
    "start-render-quiet": "cross-env NODE_ENV=development node --trace-warnings --max_old_space_size=2096 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js --progress --color --watch",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js",
    "package-all": "npm run package -- --all",
    "dev": "concurrently --raw --kill-others \"npm run start-render-quiet\" \"npm run start-main\""
  },
  "author": "Dennis Brown <npm@dvs.com.co> (https://networksynapse.net/)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.5",
    "css-loader": "0.28.11",
    "electron": "2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "node-fs-extra": "0.8.2",
    "node-sass": "4.9.0",
    "prop-types": "15.6.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.3",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "vex-js": "4.1.0",
    "webpack": "3.10.0",
    "webpack-cli": "2.1.3",
    "webpack-dashboard": "1.1.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "2.10.1",
    "webpack-hot-middleware": "2.22.1",
    "webpack-merge": "4.1.2"
  },
  "devDependencies": {
    "devtron": "1.4.0"
  }
}
