{
  "name": "blender-config-manager",
  "version": "1.0.0",
  "description": "Blender Configuration Manager",
  "main": "./build/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-main": "cross-env HOT=1 electron --enable-logging ./electron/main",
    "start-render": "cross-env NODE_ENV=development node --trace-warnings --max_old_space_size=2096 -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js --progress --color --watch",
    "start-render-quiet": "cross-env NODE_ENV=development node --trace-warnings --max_old_space_size=2096 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js --progress --color --watch",
    "bundle-electron": "node ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors --mode production",
    "bundle-frontend": "node ./node_modules/webpack/bin/webpack --config webpack.config.prod.js --progress --profile --colors --mode production",
    "bundle-pre": "node ./node_modules/webpack/bin/webpack --config webpack.config.prerelease.js --progress --profile --colors --mode production",
    "bundle": "npm run bundle-electron && npm run bundle-frontend",
    "prod": "cross-env NODE_ENV=production electron ./build/main",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "concurrently --raw --kill-others \"npm run start-render-quiet\" \"npm run start-main\""
  },
  "author": "Dennis Brown <npm@dvs.com.co> (https://networksynapse.net/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "css-loader": "0.28.11",
    "devtron": "1.4.0",
    "electron": "2.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "4.9.0",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "webpack": "4.15.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "2.1.3",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.3"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.5",
    "jest": "22.4.3",
    "node-fs-extra": "0.8.2",
    "prop-types": "15.6.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.3",
    "vex-js": "4.1.0"
  },
  "build": {
    "appId": "com.electron.blender-config-manager",
    "productName": "Blender Config Manager",
    "copyright": "Copyright 2018 Dennis Brown",
    "files": [
      "build/**/*"
    ],
    "mac": {
      "category": "public.app-category.photography"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  }
}
